      "html": "html",
      "javascript": [
        "javascriptreact",
        "css"
      ],
    },
    // Bracket_Pair
    "bracketPairColorizer.consecutivePairColors": [
      "()",
      "[]",
      "{}",
      [
        "Gold",
        "LightSeaGreen",
        "Darkred"
        // LightSkyBlue
      ],
      "Red"
    ],
    "bracketPairColorizer.colorMode": "Consecutive",
    "window.zoomLevel": 1,
    "editor.minimap.enabled": true,
    "window.menuBarVisibility": "toggle",
    "atomKeymap.promptV3Features": true,
